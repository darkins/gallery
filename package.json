{
  "name": "eleventy-photo-gallery",
  "version": "1.2.0",
  "description": "A photo gallery starter kit built with the static site generator Eleventy",
  "main": ".eleventy.js",
  "scripts": {
    "build": "npm-run-all build:*",
    "start": "npm-run-all watch:*",
    "dev": "npm-run-all --parallel watch:*",
    "build:eleventy": "npx @11ty/eleventy",
    "watch:eleventy": "npx @11ty/eleventy --serve",
    "build:sass": "sass src/_includes/sass:src/_includes/css",
    "watch:sass": "sass --watch src/_includes/sass:src/_includes/css",
    "debug": "DEBUG=* @11ty/eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tannerdolby/eleventy-photo-gallery.git"
  },
  "bugs": {
    "url": "https://github.com/tannerdolby/eleventy-photo-gallery/issues"
  },
  "author": {
    "name": "Tanner Dolby",
    "email": "tannercdolby@gmail.com",
    "url": "https://github.com/tannerdolby"
  },
  "keywords": [
    "Eleventy",
    "11ty",
    "Photo Gallery",
    "Photo Gallery Template"
  ],
  "homepage": "https://github.com/tannerdolby/eleventy-photo-gallery#readme",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-cache-assets": "^2.3.0",
    "@11ty/eleventy-navigation": "^0.3.3",
    "clean-css": "^4.2.3",
    "eleventy-plugin-metagen": "^1.7.11",
    "eleventy-plugin-sharp-respimg": "^1.3.7",
    "npm-run-all": "^4.1.5",
    "sass": "^1.39.0",
    "terser": "^5.7.2"
  }
}
